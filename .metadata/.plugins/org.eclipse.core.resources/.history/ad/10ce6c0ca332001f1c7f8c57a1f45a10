grammar upb.edu.lp.Cocinita with org.eclipse.xtext.common.Terminals

generate cocinita "http://www.edu.upb/lp/Cocinita"

Cocina:
	'[RECIPE]' name = ID
	'Ingredientes:'
	Ingrediente += ListaIngredientes*
	'Procedimiento:'
	
;

ListaIngredientes:
	ascii= ASCII '[' tipo = TipoVariable ']' name = ID
;

TipoVariable:
    Int='[-o]' | String='[cU]' | Boolean='[-O]'
;

terminal ASCII:
	'0'..'9'+
;

//IntExpression:
//	
//;